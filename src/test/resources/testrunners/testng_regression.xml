<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open Cart Regression Suite" thread-count="5"
	 verbose="4">
	<listeners>
		<listener
			class-name="listeners.AnnotationTransformer" />
		<listener
			class-name="listeners.ExtentReportListener"/>
		
	</listeners>
	<test name="Login Page Test_chrome90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90" />
		<classes>
			<class name="tests.LoginPageTest" />
		</classes>
	</test> <!-- Test -->
	<test name="Accounts Page Test_chrome90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90" />
		<classes>
			<class name="tests.AccountsPageTest" />
		</classes>
	</test>
	<test name="Product Info Page Test_chrome90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90" />
		<classes>
			<class name="tests.ProductInfoTest" />
		</classes>
	</test>
	<test name="Registeration  Page Test_chrome90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90" />
		<classes>
			<class name="tests.RegisterationPageTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
